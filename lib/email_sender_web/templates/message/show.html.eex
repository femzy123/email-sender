<h1>Show Message</h1>

<ul>

  <li>
    <strong>To:</strong>
    <%= @message.to %>
  </li>

  <li>
    <strong>Subject:</strong>
    <%= @message.subject %>
  </li>

  <li>
    <strong>Body:</strong>
    <%= @message.body %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.message_path(@conn, :edit, @message) %></span>
<span><%= link "Back", to: Routes.message_path(@conn, :index) %></span>
